package variable;

public class CharTest {
	public static void main(String[] args) {
		char ch = 'a';
		System.out.println(ch);
		System.out.println((int)ch);
		
		int ich = 66;
		System.out.println(ich);
		System.out.println((char)ich);
		
		char hangul = '한글';
		System.out.println(hangul);
		
		char ch2 = '한글';
		System.out.println(ch2);
		
	}
}
